<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".main_code.about_time_picker.TimePickerActivity">

    <include layout="@layout/activity_include_top_bar" />
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center"
        >
        <TextView
            android:id="@+id/time_picker_tv_content"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/dp10"
            />
        <TextView
            android:id="@+id/time_picker_tv_show"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_stroke_pinkf8"
            android:textColor="@color/white"
            android:padding="@dimen/dp10"
            android:layout_marginTop="@dimen/dp20"
            android:text="@string/main_btn_jump_2_time_picker_view"
            />
    </LinearLayout>

</LinearLayout>