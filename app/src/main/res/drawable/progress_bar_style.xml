<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5dip" />
            <gradient
                android:startColor="@color/greye5"
                android:centerColor="@color/greye5"
                android:centerY="0.75"
                android:endColor="@color/greye5"
                android:angle="270"
                />
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5dip" />
                <gradient
                    android:startColor="@color/app_theme"
                    android:centerColor="@color/app_theme"
                    android:centerY="0.75"
                    android:endColor="@color/app_theme"
                    android:angle="270"
                    />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <scale
            android:drawable="@drawable/selector_wallet_oc_theme"
            android:scaleWidth="100%"
            />
<!--        <clip>
            <shape>
                <corners android:radius="5dip" />
